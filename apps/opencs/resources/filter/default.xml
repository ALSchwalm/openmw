<!DOCTYPE FilterTree>
<SetOperation type="Union">
  <Name>Default filters</Name>
  <ChildItems>
    <Default active="true">
      <Name>Any</Name>
    </Default>
    
    <Match type="Wildcard" active="false">
      <Name>Flora</Name>
      <Key>mwId</Key>
      <Value>*flora*</Value>
    </Match>

    <SetOperation type="Intersection" active="false">
      <Name>Activators with Sound</Name>
      <ChildItems>
      <Match type="Exact">
        <Name>Activator</Name>
        <Key>mwType</Key>
        <Value>ACTI</Value>
      </Match>
        <Match type="Regex">
        <Name>Sound</Name>
        <Key>script</Key>
        <Value>Sound.*</Value>
      </Match>
      </ChildItems>
    </SetOperation>
  </ChildItems>
</SetOperation>
