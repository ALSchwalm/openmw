<!DOCTYPE FilterTree>
<SetOperation type="Union">
  <Name>Default filters</Name>
  <ChildItems>
    <Default active="false">
      <Name>Any</Name>
    </Default>
    
    <Match type="Wildcard" active="false">
      <Name>Smoke</Name>
      <Key>mwId</Key>
      <Value>*smoke*</Value>
    </Match>
    
    <SetOperation type="Union">
      <Name>Record Types</Name>
      <ChildItems>
        <Match type="Exact" active="false">
          <Name>Game Setting</Name>
          <Key>mwType</Key>
          <Value>GMST</Value>
        </Match>
        <Match type="Exact" active="false">
          <Name>Activator</Name>
          <Key>mwType</Key>
          <Value>ACTI</Value>
        </Match>
        <Match type="Exact">
          <Name>Potion</Name>
          <Key>mwType</Key>
          <Value>ALCH</Value>
        </Match>
        <Match type="Exact" active="false">
          <Name>Script</Name>
          <Key>mwType</Key>
          <Value>SCPT</Value>
        </Match>
      </ChildItems>
    </SetOperation>
    
    <SetOperation type="Intersection" active="false">
      <Name>Activators with Sound</Name>
      <ChildItems>
      <Match type="Exact">
        <Name>Activator</Name>
        <Key>mwType</Key>
        <Value>ACTI</Value>
      </Match>
        <Match type="Regex">
        <Name>Sound</Name>
        <Key>script</Key>
        <Value>Sound.*</Value>
      </Match>
      </ChildItems>
    </SetOperation>
  </ChildItems>
</SetOperation>

