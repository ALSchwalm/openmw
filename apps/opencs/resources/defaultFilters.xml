<!DOCTYPE FilterTree>
<Union>
    <Default active="false"/>
    <Match type="Wildcard" active="false">
        <Key>mwId</Key>
        <Value>*smoke*</Value>
    </Match>
    <Union>
        <Match type="Exact" active="false">
            <Key>mwType</Key>
            <Value>ACTI</Value>
        </Match>
        <Match type="Exact">
            <Key>mwType</Key>
            <Value>ALCH</Value>
        </Match>
        <Match type="Exact" active="false">
            <Key>mwType</Key>
            <Value>SCPT</Value>
        </Match>
    </Union>
</Union>

